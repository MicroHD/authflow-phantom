{
  "name": "@auth-flow/phantom",
  "version": "0.2.3",
  "description": "Invisible, Secure, Effortless Login â€“ Built for Modern TypeScript Apps",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "authflow-phantom": "./dist/cli.js"
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/*"]
    }
  },
  "files": [
    "dist",
    "prisma",
    "src",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "CODE_OF_CONDUCT.md",
    "SECURITY.md"
  ],
  "scripts": {
    "build": "tsc --declaration --emitDeclarationOnly && tsc --module commonjs --outDir dist/cjs && tsc --module ESNext --outDir dist/esm && node scripts/fix-exports.js && cp src/cli.ts dist/cli.js && chmod +x dist/cli.js",
    "prepare": "bun run build",
    "prepublishOnly": "bun run test",
    "start": "node dist/main.js",
    "dev": "bun run --watch src/main.ts",
    "start:dev": "bun run --watch src/main.ts",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:cov": "jest --config jest.config.ts --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.config.ts --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepack": "bun run build"
  },
  "keywords": [
    "authentication",
    "passwordless",
    "typescript",
    "nestjs",
    "phantom-links",
    "webauthn",
    "magic-links",
    "security"
  ],
  "author": "AuthFlow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MicroHD/authflow-phantom"
  },
  "bugs": {
    "url": "https://github.com/MicroHD/authflow-phantom/issues"
  },
  "homepage": "https://github.com/MicroHD/authflow-phantom#readme",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/common": "^10.4.17",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.17",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.8.2",
    "@simplewebauthn/server": "^13.1.1",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "apollo-server-express": "^3.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "graphql": "^16.8.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "redis": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@nestjs/testing": "^11.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "prisma": "^6.8.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  }
}